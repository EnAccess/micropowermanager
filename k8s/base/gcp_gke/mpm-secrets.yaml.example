apiVersion: v1
kind: Secret
metadata:
  name: mpm-secret-backend
type: Opaque
stringData:
  APP_KEY: app-key
  JWT_SECRET: jwt-secret
  DB_HOST: mysql-host
  DB_USERNAME: user-name
  DB_PASSWORD: db-password
  MPM_FRONTEND_URL: FRONTEND_URL 
  LOG_SLACK_WEBHOOK_URL: slack-webhook-url
  HORIZON_SLACK_WEBHOOK_URL: slack-webhook-url
  # Google Cloud Storage Configuration
  GOOGLE_CLOUD_KEY_JSON: your-service-account-key-json-string
  GOOGLE_CLOUD_STORAGE_BUCKET= your-google-cloud-storage-bucket-name
  GOOGLE_CLOUD_PROJECT_ID= your-google-cloud-project-id
---
apiVersion: v1
kind: Secret
metadata:
  name: mpm-secret-frontend
type: Opaque
stringData:
  MPM_BACKEND_URL: BACKEND_URL
