{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "africastalking/africastalking": "^3.0",
    "barryvdh/laravel-dompdf": "^v3.1.1",
    "barryvdh/laravel-ide-helper": "^3.6",
    "bogdaan/viber-bot-php": "^0.0.15",
    "fakerphp/faker": "^1.24",
    "guzzlehttp/guzzle": "^7.10",
    "hashids/hashids": "^5.0",
    "laravel/framework": "^12.0",
    "laravel/helpers": "^1.2",
    "laravel/horizon": "^5.0",
    "laravel/slack-notification-channel": "^3.6",
    "laravel/telescope": "^5.16",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.0",
    "lcobucci/jwt": "^4.3",
    "league/flysystem-google-cloud-storage": "^3.0",
    "parsecsv/php-parsecsv": "^1.3",
    "phpoffice/phpspreadsheet": "^1.7",
    "predis/predis": "^1.1.7",
    "pusher/pusher-php-server": "^7.0",
    "spatie/geocoder": "^3.14",
    "spatie/laravel-backup": "^9.0",
    "spatie/laravel-google-cloud-storage": "^2.3",
    "spatie/laravel-permission": "^6.21",
    "spatie/laravel-query-builder": "^6.3",
    "tymon/jwt-auth": "^2.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.5",
    "driftingly/rector-laravel": "^2.0",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.64",
    "knuckleswtf/scribe": "^5.3",
    "larastan/larastan": "^3.0",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^8.1",
    "phpstan/phpstan": "^2.0",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "phpunit/phpunit": "^11.0",
    "recca0120/laravel-erd": "^0.4",
    "rector/rector": "^2.1",
    "spatie/laravel-export": "^1.0",
    "spatie/laravel-ignition": "^2.0",
    "spatie/laravel-web-tinker": "^1.7"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "lint": "find . -name \\*.php -not -path './lib/composer/*' -not -path './vendor/*' -not -path './build/.phan/*' -exec php -l \"{}\" \\;",
    "php-cs-fixer-fix": "php-cs-fixer fix --diff",
    "php-cs-fixer-check": "php-cs-fixer check --diff",
    "php-cs-fixer-version": "php-cs-fixer --version",
    "larastan": "phpstan analyse --memory-limit=2G --no-progress",
    "larastan-next": "phpstan analyse --memory-limit=2G --no-progress --level=7",
    "larastan-no-cache": "vendor/bin/phpstan clear-result-cache && phpstan analyse --memory-limit=2G --no-progress",
    "rector-fix": "vendor/bin/rector && php-cs-fixer fix",
    "rector-check": "vendor/bin/rector --dry-run",
    "rector-version": "vendor/bin/rector --version",
    "quality:check": [
      "@php-cs-fixer-check",
      "@larastan",
      "@rector-check"
    ]
  }
}
